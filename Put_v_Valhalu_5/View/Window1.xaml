<Window x:Class="Put_v_Valhalu_5.View.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Put_v_Valhalu_5.View"
        mc:Ignorable="d"
        DataContext = "{Binding SecondViewModel}"
        Title="Window1" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition />





        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>



        </Grid.ColumnDefinitions>

        <ListBox Grid.Row="0" Grid.RowSpan="2" Grid.Column="0" Grid.ColumnSpan="3"  ItemsSource="{Binding SongsList}"  />
        <Button Grid.Column="4" Content="След окно" Command="{Binding SledSled}"/>
        <Button Grid.Column="5" Content="На предыдуее окно" Command="{Binding NazadNazad }"/>
        <Button Grid.Column="3" Content="Выгрузка" Command="{Binding Vugruz}"/>
    </Grid>
</Window>
